				<cachedPrior>
					<gammaPrior shape="1.0" scale="1.0" offset="0.0">
						<parameter idref="Region.rates"/>
					</gammaPrior>
					<parameter idref="Region.rates"/>
				</cachedPrior>
				<coalescentLikelihood idref="coalescent"/>
				
				
				<discretizedBranchRates idref="default.branchRates"/>
				<strictClockBranchRates idref="Region.branchRates"/>

				<!-- START Discrete Traits Model                                             -->
				<generalSubstitutionModel idref="Region.model"/>

				<!-- END Discrete Traits Model                                               -->

			</prior>
			<likelihood id="likelihood">
				<treeDataLikelihood idref="default.treeLikelihood"/>

				<!-- START Discrete Traits Model                                             -->
				<markovJumpsTreeLikelihood idref="Region.treeLikelihood"/>

				<!-- END Discrete Traits Model                                               -->

			</likelihood>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="10000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="age(root)" sf="6" width="12">
				<tmrcaStatistic idref="age(root)"/>
			</column>
			<column label="default.uced.mean" sf="6" width="12">
				<parameter idref="default.uced.mean"/>
			</column>
			<column label="Region.clock.rate" sf="6" width="12">
				<parameter idref="Region.clock.rate"/>
			</column>

			<!-- START Discrete Traits Model                                             -->
			<column label="Region.nonZeroRates" dp="0" width="6">
				<sumStatistic idref="Region.nonZeroRates"/>
			</column>

			<!-- END Discrete Traits Model                                               -->

		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="10000" fileName="Kappa_Ex_UCED_Sym2.log" overwrite="false">
			<joint idref="joint"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<tmrcaStatistic idref="age(root)"/>
			<treeLengthStatistic idref="treeLength"/>
			<tmrcaStatistic idref="tmrca(GCC)"/>
			<tmrcaStatistic idref="age(GCC)"/>
			<parameter idref="exponential.popSize"/>
			<parameter idref="exponential.growthRate"/>
			<parameter idref="gtr.rates"/>
			<parameter idref="frequencies"/>
			<parameter idref="alpha"/>
			<parameter idref="default.uced.mean"/>
			<parameter idref="Region.clock.rate"/>
			<rateStatistic idref="default.meanRate"/>
			<rateStatistic idref="default.coefficientOfVariation"/>
			<rateCovarianceStatistic idref="default.covariance"/>
			<rateStatistic idref="Region.meanRate"/>

			<!-- START Ancestral state reconstruction                                    -->

			<!-- END Ancestral state reconstruction                                      -->


			<!-- START Discrete Traits Model                                             -->
			<parameter idref="Region.rates"/>
			<parameter idref="Region.indicators"/>
			<sumStatistic idref="Region.nonZeroRates"/>

			<!-- END Discrete Traits Model                                               -->

			<treeDataLikelihood idref="default.treeLikelihood"/>
			<discretizedBranchRates idref="default.branchRates"/>
			<strictClockBranchRates idref="Region.branchRates"/>

			<!-- START Discrete Traits Model                                             -->
			<markovJumpsTreeLikelihood idref="Region.treeLikelihood"/>

			<!-- END Discrete Traits Model                                               -->

			<coalescentLikelihood idref="coalescent"/>
			
		</log>

		<!-- START Discrete Traits Model                                             -->
		<log id="Kappa_Ex_UCED_Sym2.RegionrateMatrixLog" logEvery="10000" fileName="Kappa_Ex_UCED_Sym2.Region.rates.log">
			<parameter idref="Region.rates"/>
			<parameter idref="Region.indicators"/>
			<sumStatistic idref="Region.nonZeroRates"/>
		</log>

		<!-- END Discrete Traits Model                                               -->


		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="10000" nexusFormat="true" fileName="Kappa_Ex_UCED_Sym2.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<trait name="rate" tag="default.rate">
				<discretizedBranchRates idref="default.branchRates"/>
			</trait>
			<trait name="rate" tag="Region.rate">
				<strictClockBranchRates idref="Region.branchRates"/>
			</trait>
			<joint idref="joint"/>

			<!-- START Ancestral state reconstruction                                    -->
			<trait name="Region.states" tag="Region">
				<ancestralTreeLikelihood idref="Region.treeLikelihood"/>
			</trait>
			<!-- END Ancestral state reconstruction                                      -->

		</logTree>
	</mcmc>
	

	<!-- START Marginal Likelihood Estimator                                     -->

	<!-- Define marginal likelihood estimator (PS/SS) settings                   -->
	<marginalLikelihoodEstimator chainLength="1000000" pathSteps="100" pathScheme="betaquantile" alpha="0.3">
		<samplers>
			<mcmc idref="mcmc"/>
		</samplers>
		<pathLikelihood id="pathLikelihood">
			<source>
				<joint idref="joint"/>
			</source>
			<destination>
				<prior idref="prior"/>
			</destination>
		</pathLikelihood>
		<log id="MLELog" logEvery="1000" fileName="Kappa_Ex_UCED_Sym2.mle.log">
			<pathLikelihood idref="pathLikelihood"/>
		</log>
	</marginalLikelihoodEstimator>

	<!-- Path sampling estimator from collected samples                          -->
	<pathSamplingAnalysis fileName="Kappa_Ex_UCED_Sym2.mle.log" resultsFileName="Kappa_Ex_UCED_Sym2.mle.result.log">
		<likelihoodColumn name="pathLikelihood.delta"/>
		<thetaColumn name="pathLikelihood.theta"/>
	</pathSamplingAnalysis>

	<!-- Stepping-stone sampling estimator from collected samples                -->
	<steppingStoneSamplingAnalysis fileName="Kappa_Ex_UCED_Sym2.mle.log" resultsFileName="Kappa_Ex_UCED_Sym2.mle.result.log">
		<likelihoodColumn name="pathLikelihood.delta"/>
		<thetaColumn name="pathLikelihood.theta"/>
	</steppingStoneSamplingAnalysis>

	<!-- END Marginal Likelihood Estimator                                       -->

	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
	
</beast>
